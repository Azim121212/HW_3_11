<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="FoughActivity">

    <TextView
        android:paddingStart="12dp"
        android:paddingTop="12dp"
        android:background="@drawable/background"
        android:id="@+id/fourthBottom"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/fourthCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="286dp"
        android:layout_height="46dp"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:paddingStart="12dp"
        android:paddingTop="4dp"
        android:background="@drawable/background"
        android:id="@+id/fourthCenter"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/fourthResult"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="286dp"
        android:layout_height="46dp"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:paddingTop="4dp"
        android:background="@drawable/background"
        android:paddingStart="16dp"
        android:id="@+id/fourthResult"
        android:layout_marginTop="56dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="286dp"
        android:layout_height="46dp"
        tools:ignore="RtlSymmetry" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/fourthEnd"
        android:text="@string/end"
        android:layout_marginEnd="18dp"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@id/fourthBottom"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="122dp"
        android:layout_height="56dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>